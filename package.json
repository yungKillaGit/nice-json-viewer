{
  "name": "nice-json-viewer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "A fast, clean JSON viewer for React and Next.js. Paste your JSON and get a readable, formatted output instantly.",
  "keywords": [
    "json",
    "viewer",
    "react",
    "nextjs",
    "formatter",
    "pretty",
    "parser",
    "visualizer"
  ],
  "homepage": "https://github.com/yungKillaGit/nice-json-viewer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yungKillaGit/nice-json-viewer.git"
  },
  "license": "MIT",
  "volta": {
    "node": "24.11.1"
  },
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build",
    "start": "next start",
    "preview": "npm run clear-cache && npm run build && npm run start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean-install": "rimraf .next node_modules package-lock.json && npm i",
    "clear-cache": "rimraf .next",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "vitest run --project unit",
    "test:storybook": "vitest run --project storybook",
    "test": "vitest run",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npx vitest run || echo \"No test files found, skipping tests\""
    ]
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.555.0",
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-json-view-lite": "^2.5.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^2.0.0",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-vitest": "^10.1.4",
    "@storybook/nextjs-vite": "^10.1.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^10.1.4",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rimraf": "^6.1.2",
    "storybook": "^10.1.4",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  }
}
